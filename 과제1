#include <stdio.h>
#include <iostream>
using namespace std;

class CRect{
private:
    int x,y;
public:
    CRect(int x,int y){
        this->x = x;
        this->y = y;
    }
    CRect(){
        this->x = 0;
        this->y = 0;
    }
    void result(){
       cout << x << " " << y << endl; 
    }
    CRect operator+(CRect r){
        CRect t;
        t.x = x + r.x;
        t.y = y + r.y;
        return(t);
    }
};

int main(){
    CRect p1(2,3), p2(4,5) , p3;
    //p3 = p1.operator+(p2);
    p3 = p2 + p1;
    p3.result();
    return 0;
}
